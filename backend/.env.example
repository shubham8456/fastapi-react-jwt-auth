# Backend (.env.example)
# Backend config values read by app/core/config.py (Pydantic BaseSettings)

# Secret key used to sign JWTs
JWT_SECRET_KEY=supersecretkey

# Cookie flags (development defaults)
COOKIE_SECURE=false
COOKIE_SAMESITE=lax
# COOKIE_DOMAIN= when set should match your host (e.g. example.com) or be empty for localhost

ALLOWED_HOSTS=http://localhost:3000
